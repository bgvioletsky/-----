[Rule]
DOMAIN-SUFFIX,jsdelivr.net,PROXY

[Script]
http-request ^https?:\/\/(.+\.)?xs\.(com|net) script-path=https://cdn.jsdelivr.net/gh/bgvioletsky/XS_Resource@0.0.4/conf/xs.js, requires-body=true, timeout=120, tag=xs

[MITM]
hostname = xs.com, xs.net, *.xs.com, *.xs.net
